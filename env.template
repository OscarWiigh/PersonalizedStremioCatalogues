# Trakt API Configuration
# Get your API keys at: https://trakt.tv/oauth/applications
TRAKT_CLIENT_ID=your_trakt_client_id_here
TRAKT_CLIENT_SECRET=your_trakt_client_secret_here
TRAKT_USERNAME=your_trakt_username

# TMDB API Configuration
# Get your API key at: https://www.themoviedb.org/settings/api
TMDB_API_KEY=your_tmdb_api_key_here

# FlixPatrol API Configuration (Optional - NOT NEEDED!)
# Netflix Top 10 data is fetched from Netflix's official free API
# FlixPatrol is only used as a backup if configured
# FLIXPATROL_API_KEY=your_flixpatrol_api_key_here

# Server Configuration
PORT=7000

